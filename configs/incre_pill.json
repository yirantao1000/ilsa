    "algo": {
        "task": "pill",
        "latest": true,
        "loc_only": true,
        
        "finish":true,
        "finish_separate":true,
        "finish_weight":10,
        "finish_layer_dims": [512,512],

        "real":true,
        "limit": true,


        "wrong_direction_loss": 200.0,
        "squared_wrong_direction_loss":0,
    

        "major_direction_loss":200.0,
        "squared_major_direction_loss":0,

        "incremental":true,
        "partial_incremental": false,
