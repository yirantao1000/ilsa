        "middle_gt_all": true,

        "middle_gt_wrong": true,
        "final_gt_wrong": true,
        
        "gt_weight":1,
        "user_weight":1,
        "wrong_user_loss": false,
        "middle_gt_weight":1,

           
        "optim_params": {
            "policy": {
                "optimizer_type": "adamw",
                "learning_rate": {
                    "initial": 0.0001,
                    "decay_factor": 0.1,
                    "epoch_schedule": [100],
                    "scheduler_type": "linear"
